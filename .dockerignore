.env
.env.development
.env.local
venv/
env/
__pycache__/
*.pyc
*.pyo
*.pyd
.Python
.pytest_cache/

# IDE設定
.vscode/
.idea/
*.swp
*.swo
*~

# ログファイル
logs/
*.log

# 一時ファイル
tmp/
temp/
.tmp

# バックアップ
backups/
*.backup
*.bak

# テストファイル
test_*
*_test.py
tests/

